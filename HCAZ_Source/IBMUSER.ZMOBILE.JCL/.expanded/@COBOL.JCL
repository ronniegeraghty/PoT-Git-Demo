XXSYSUT11  DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT12  DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT13  DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT14  DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT15  DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSPRINT DD SYSOUT=*
XX*-------------------------------------------------------------------*
XX*  REBLOCK DFHEILIA, FOR USE BY THE LINKEDIT STEP                   *
XX*-------------------------------------------------------------------*
XX*
XXCOPYLINK EXEC PGM=IEBGENER,COND=(7,LT,COBL)
XXSYSUT1   DD DISP=SHR,DSN=DFH520.CICS.SDFHCOB(DFHEILIC)
XXSYSUT2   DD DISP=(NEW,PASS),DSN=&&COPYLINK,
XX            DCB=(LRECL=80,BLKSIZE=3120,RECFM=FB),
XX            UNIT=SYSDA,SPACE=(CYL,(1,1))
XXSYSPRINT DD DUMMY
XXSYSIN    DD DUMMY
XX*-------------------------------------------------------------------*
XX*  INVOKE THE MVS LINKAGE-EDITOR PROGRAM                            *
XX*-------------------------------------------------------------------*
XX*
XXLKED     EXEC PGM=HEWL,COND=(7,LT,COBL),
XX  PARM='LIST,XREF,RENT,NAME=HCAZMENU'
XXSYSLIB   DD DISP=SHR,DSN=DFH520.CICS.SDFHLOAD
XX         DD DSN=DSNB10.SDSNLOAD,DISP=SHR
XX         DD DSN=CEE.SCEELKED,DISP=SHR
XXSYSLMOD  DD DISP=SHR,DSN=DFH520.CICSA.LOAD(HCAZMENU)
XXSYSUT1   DD UNIT=SYSDA,DCB=BLKSIZE=1024,
XX            SPACE=(CYL,(1,1))
XXSYSPRINT DD SYSOUT=*
XXSYSLIN   DD DISP=(OLD,DELETE),DSN=&&COPYLINK
XX         DD DISP=(OLD,DELETE),DSN=&&LOADSET
XX         DD DISP=SHR,DSN=IBMUSER.ZMOBILE.JCL(LINKPARM)
XX         PEND
