XXDB2PROC  PROC
XX*-------------------------------------------------------------------*
XX*  INVOKE THE COBOL COMPILE                                         *
XX*-------------------------------------------------------------------*
XX*
XXCOBL     EXEC PGM=IGYCRCTL,
XX PARM='NODYNAM,LIB,RENT,APOST,LIB,CICS('SP'),SQL,SIZE(4000K)'
XXSTEPLIB  DD DSN=IGY510.SIGYCOMP,DISP=SHR
XX         DD DSN=DFH520.CICS.SDFHLOAD,DISP=SHR
XX*         DD DSN=&DSNAHLQ..SDSNEXIT,DISP=SHR
XX         DD DSN=DSNB10.SDSNEXIT,DISP=SHR
XX*         DD DSN=&DSNAHLQ..RUNLIB.LOAD,DISP=SHR
XX         DD DSN=DSNB10.SDSNLOAD,DISP=SHR
XXSYSLIB   DD DSN=DFH520.CICS.SDFHCOB,DISP=SHR
XX         DD DSN=DFH520.CICS.SDFHMAC,DISP=SHR
XX         DD DSN=DFH520.CICS.SDFHSAMP,DISP=SHR
XX         DD DSN=DSNB10.SDSNSAMP,DISP=SHR
XX         DD DSN=DSNB10.SDSNMACS,DISP=SHR
XX         DD DSN=IBMUSER.ZMOBILE.COPYLIB,DISP=SHR
XXSYSIN    DD DISP=SHR,DSN=IBMUSER.ZMOBILE.COBOL(HCAZMENU)
XXDBRMLIB  DD DSN=IBMUSER.ZMOBILE.DBRMLIB(HCAZMENU),DISP=SHR
XXSYSLIN   DD DSN=&&LOADSET,DISP=(NEW,PASS),UNIT=SYSDA,
XX         SPACE=(TRK,(20,10))
XXSYSADATA DD DUMMY
XXSYSMDECK DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT1   DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT2   DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT3   DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT4   DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT5   DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT6   DD UNIT=SYSDA,SPACE=(460,(350,100))
XXSYSUT7   DD UNIT=SYSDA,SPACE=(460,(350,100))
